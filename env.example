# Environment Configuration
# Copy this file to .env and fill in your values

# Application Environment
NODE_ENV=development

# Database Configuration
# For local Docker development, use the PostgreSQL container:
DATABASE_URL=postgresql://turborepo:123456789@localhost:5435/turborepo

# Redis Configuration (Optional)
# For local Docker development, use the Redis container:
REDIS_URL=redis://localhost:6379

# Error Monitoring (Optional)
SENTRY_DSN=

# Analytics Configuration (Optional)
# PostHog Configuration for the www app
VITE_PUBLIC_POSTHOG_KEY=
VITE_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Bot Configuration
BOT_TOKEN=your_discord_bot_token_here
BOT_ENABLED=false
STATUS_CHANNEL_ID=1322369714701205543
STATUS_MESSAGE_ID=1417226755894284368
STATUS_URL=https://status.deadlockmods.app/
STATUS_INTERVAL_MIN=5
STATUS_SELECTOR=main
STATUS_PIN=true
STATUS_ENABLED=true